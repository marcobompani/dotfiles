alias youtubetomp3='youtube-dl --extract-audio --audio-format mp3'
alias keyboard='setxkbmap -option grp:alt_shift_toggle'
alias gdb='gdb --quiet'
alias rm='trash'
alias ls='exa --color always'
alias ll='exa -alF --color always'
alias la='exa -a --color always'
alias lct='exa --sort changed --reverse'
alias vi='nvim'
alias vim='nvim'
alias nano='nvim'
alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'
